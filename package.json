{
  "name": "teoclits",
  "version": "0.0.1",
  "description": "Typescript angular Teonet Client module",
  "main": "teocli.ts",
  "directories": {
    "test": "test"
  },
  "dependencies": {
    "@angular/common": "^4.4.6",
    "@angular/core": "^4.4.6",
    "rxjs": "^5.5.2",
    "teocli": "^0.0.14"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "transpile": "ngc",
    "package": "rollup -c",
    "minify": "uglifyjs dist/bundles/teocli.umd.js --screw-ie8 --compress --mangle --comments --output dist/bundles/teocli.umd.min.js",
    "build": "npm run transpile && npm run package && npm run minify && cp package_dist.json dist/package.json"
  },
  "repository": {
    "type": "git",
    "url": "https://gitlab.ksproject.org/teonet/TeocliTS.git"
  },
  "keywords": [
    "teonet",
    "network"
  ],
  "author": "Kirill Scherba <kirill@scherba.ru> (http://gitlab.ksproject.org/)",
  "license": "MIT",
  "devDependencies": {
    "@angular/compiler": "^4.4.6",
    "@angular/compiler-cli": "^4.4.6",
    "rollup": "^0.50.0",
    "typescript": "^2.5.3",
    "uglify-js": "^3.1.6"
  }
}
